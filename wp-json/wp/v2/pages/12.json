verlay\r\n    const modalOverlays = document.querySelectorAll('.cta-modal-overlay');\r\n    modalOverlays.forEach(function(overlay) {\r\n        overlay.addEventListener('click', function(e) {\r\n            if (e.target === this) {\r\n                this.style.display = 'none';\r\n                document.body.style.overflow = ''; \/\/ Restore scrolling\r\n            }\r\n        });\r\n    });\r\n    \r\n    \/\/ Close modal with Escape key\r\n    document.addEventListener('keydown', function(e) {\r\n        if (e.key === 'Escape') {\r\n            const visibleModals = document.querySelectorAll('.cta-modal-overlay[style*=\"flex\"]');\r\n            visibleModals.forEach(function(modal) {\r\n                modal.style.display = 'none';\r\n                document.body.style.overflow = ''; \/\/ Restore scrolling\r\n            });\r\n        }\r\n    });\r\n});\r\n<\/script>\n\n\r\n<div id=\"fifty-fifty-block_79f6d2cc8315d98db891f8f14f01ada6\" class=\"block site_width related-content\">\r\n\r\n            \r\n        <div class=\"fifty_fifty_cards_grid\">\r\n            <article class=\"single_content_card image_left\">\r\n                <div class=\"content_thumbnail\">\r\n                    <img loading=\"lazy\" decoding=\"async\" width=\"450\" height=\"300\" src=\"https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/05\/HSAOpeningEstrado5-450x300.jpg\" class=\"attachment-medium size-medium\" alt=\"\" srcset=\"https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/05\/HSAOpeningEstrado5-450x300.jpg 450w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/05\/HSAOpeningEstrado5-1200x800.jpg 1200w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/05\/HSAOpeningEstrado5-768x512.jpg 768w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/05\/HSAOpeningEstrado5-1536x1025.jpg 1536w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/05\/HSAOpeningEstrado5-2048x1366.jpg 2048w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/05\/HSAOpeningEstrado5-1920x1281.jpg 1920w\" sizes=\"auto, (max-width: 450px) 100vw, 450px\" \/>                <\/div>\r\n                <div class=\"content_card_info\">\r\n                    <div class=\"content_card_excerpt\">\r\n                        <h1>Tours at Two<\/h1>\n<p>Explore highlights of Hispanic Sociaty\u2019s history, works on view, special exhibitions, and the Audubon Terrace with Museum educators\u00a0Friday and Saturday at 2:00 p.m.<\/p>\n                    <\/div>\r\n                                            <div class=\"content_card_ctas\">\r\n                                                            <a href=\"https:\/\/hispanicsociaty.org\/whats-on\/guided-tours\/\" class=\"cta_button small accent\">\r\n                                    Learn More                                <\/a>\r\n                                                                                    <\/div>\r\n                        <\/div>\r\n                                    <\/div>\r\n            <\/article>\r\n        <\/div>\r\n\r\n    \r\n<\/div>\r\n\n\n\r\n<div id=\"fifty-fifty-block_7c102116b48da85a0d266d3c8d4ae2fb\" class=\"block site_width related-content\">\r\n\r\n            \r\n        <div class=\"fifty_fifty_cards_grid\">\r\n            <article class=\"single_content_card image_right\">\r\n                <div class=\"content_thumbnail\">\r\n                    <img loading=\"lazy\" decoding=\"async\" width=\"450\" height=\"321\" src=\"https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/Concert-1-450x321.jpg\" class=\"attachment-medium size-medium\" alt=\"\" srcset=\"https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/Concert-1-450x321.jpg 450w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/Concert-1-1200x857.jpg 1200w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/Concert-1-768x548.jpg 768w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/Concert-1-1536x1097.jpg 1536w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/Concert-1-2048x1463.jpg 2048w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/Concert-1-1920x1371.jpg 1920w\" sizes=\"auto, (max-width: 450px) 100vw, 450px\" \/>                <\/div>\r\n                <div class=\"content_card_info\">\r\n                    <div class=\"content_card_excerpt\">\r\n                        <h1>Concert Series<\/h1>\n<p>The Hispanic Sociaty Concert Series has welcomed some of the world\u2019s finest artists, chamber groups, and modern music ensembles who have performed programs highlighting works by composers from across the Hispanic world.<\/p>\n                    <\/div>\r\n                                            <div class=\"content_card_ctas\">\r\n                                                            <a href=\"https:\/\/hispanicsociaty.org\/whats-on\/concert-series\/\" class=\"cta_button small accent\">\r\n                                    Learn more                                <\/a>\r\n                                                                                    <\/div>\r\n                        <\/div>\r\n                                    <\/div>\r\n            <\/article>\r\n        <\/div>\r\n\r\n    \r\n<\/div>\r\n\n\n\r\n<div id=\"fifty-fifty-block_b4fc3bd050c2dc2a044562735e5e36bd\" class=\"block site_width related-content\">\r\n\r\n            \r\n        <div class=\"fifty_fifty_cards_grid\">\r\n            <article class=\"single_content_card image_left\">\r\n                <div class=\"content_thumbnail\">\r\n                    <img loading=\"lazy\" decoding=\"async\" width=\"338\" height=\"450\" src=\"https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/IMG_6950-Teresa-Figueiras-338x450.jpg\" class=\"attachment-medium size-medium\" alt=\"\" srcset=\"https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/IMG_6950-Teresa-Figueiras-338x450.jpg 338w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/IMG_6950-Teresa-Figueiras-900x1200.jpg 900w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/IMG_6950-Teresa-Figueiras-768x1024.jpg 768w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/IMG_6950-Teresa-Figueiras-1152x1536.jpg 1152w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/IMG_6950-Teresa-Figueiras-1536x2048.jpg 1536w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/IMG_6950-Teresa-Figueiras-1920x2560.jpg 1920w\" sizes=\"auto, (max-width: 338px) 100vw, 338px\" \/>                <\/div>\r\n                <div class=\"content_card_info\">\r\n                    <div class=\"content_card_excerpt\">\r\n                        <h1>Hispanic Heritage Month<\/h1>\n<p>Each year during Hispanic Heritage Month the Hispanic Sociaty presents an exciting slate of programming to honor the cultural legacies of Hispanic communities here in the United States and across the world.<\/p>\n                    <\/div>\r\n                                            <div class=\"content_card_ctas\">\r\n                                                            <a href=\"https:\/\/hispanicsociaty.org\/whats-on\/hispanic-heritage-month\/\" class=\"cta_button small accent\">\r\n                                    Learn More                                <\/a>\r\n                                                                                    <\/div>\r\n                        <\/div>\r\n                                    <\/div>\r\n            <\/article>\r\n        <\/div>\r\n\r\n    \r\n<\/div>\r\n\n\n\r\n<div id=\"fifty-fifty-block_f602e56c038b57b3ef0592e19bf5426d\" class=\"block site_width related-content\">\r\n\r\n            \r\n        <div class=\"fifty_fifty_cards_grid\">\r\n            <article class=\"single_content_card image_right\">\r\n                <div class=\"content_thumbnail\">\r\n                    <img loading=\"lazy\" decoding=\"async\" width=\"450\" height=\"300\" src=\"https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/05\/HSATerrace11-450x300.jpg\" class=\"attachment-medium size-medium\" alt=\"\" srcset=\"https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/05\/HSATerrace11-450x300.jpg 450w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/05\/HSATerrace11-1200x800.jpg 1200w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/05\/HSATerrace11-768x512.jpg 768w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/05\/HSATerrace11-1536x1025.jpg 1536w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/05\/HSATerrace11-2048x1366.jpg 2048w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/05\/HSATerrace11-1920x1281.jpg 1920w\" sizes=\"auto, (max-width: 450px) 100vw, 450px\" \/>                <\/div>\r\n                <div class=\"content_card_info\">\r\n                    <div class=\"content_card_excerpt\">\r\n                        <h1>Contemporary Art Initiatives<\/h1>\n<p>The Hispanic Sociaty has a long history of exhibiting contemporary art going back to its founding. Carrying on that legacy, the museum regularly invites local and international contemporary artists to dialogue with our historic collections.<\/p>\n                    <\/div>\r\n                                            <div class=\"content_card_ctas\">\r\n                                                            <a href=\"https:\/\/hispanicsociaty.org\/whats-on\/contemporary-art-initiatives\/\" class=\"cta_button small accent\">\r\n                                    Learn More                                <\/a>\r\n                                                                                    <\/div>\r\n                        <\/div>\r\n                                    <\/div>\r\n            <\/article>\r\n        <\/div>\r\n\r\n    \r\n<\/div>\r\n\n\n\r\n<div id=\"related-content-block_aabc71ee183b2711cee658c883558623\" class=\"block site_width related-content\">\r\n    <h3 class=\"related-content__heading\">See Also