cking on overlay\r\n    const modalOverlays = document.querySelectorAll('.cta-modal-overlay');\r\n    modalOverlays.forEach(function(overlay) {\r\n        overlay.addEventListener('click', function(e) {\r\n            if (e.target === this) {\r\n                this.style.display = 'none';\r\n                document.body.style.overflow = ''; \/\/ Restore scrolling\r\n            }\r\n        });\r\n    });\r\n    \r\n    \/\/ Close modal with Escape key\r\n    document.addEventListener('keydown', function(e) {\r\n        if (e.key === 'Escape') {\r\n            const visibleModals = document.querySelectorAll('.cta-modal-overlay[style*=\"flex\"]');\r\n            visibleModals.forEach(function(modal) {\r\n                modal.style.display = 'none';\r\n                document.body.style.overflow = ''; \/\/ Restore scrolling\r\n            });\r\n        }\r\n    });\r\n});\r\n<\/script>\n\n<div class=\"wp-block-image\">\n<figure class=\"aligncenter size-large\"><img loading=\"lazy\" decoding=\"async\" width=\"996\" height=\"1200\" src=\"https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/Corporate-Sponsors-Logo-Grid-9-25-996x1200.jpg\" alt=\"\" class=\"wp-image-3063\" srcset=\"https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/Corporate-Sponsors-Logo-Grid-9-25-996x1200.jpg 996w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/Corporate-Sponsors-Logo-Grid-9-25-373x450.jpg 373w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/Corporate-Sponsors-Logo-Grid-9-25-768x925.jpg 768w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/Corporate-Sponsors-Logo-Grid-9-25-1275x1536.jpg 1275w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/Corporate-Sponsors-Logo-Grid-9-25-1700x2048.jpg 1700w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/Corporate-Sponsors-Logo-Grid-9-25-1920x2314.jpg 1920w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/09\/Corporate-Sponsors-Logo-Grid-9-25.jpg 2000w\" sizes=\"auto, (max-width: 996px) 100vw, 996px\" \/><\/figure><\/div>\n\n\r\n<div id=\"centered-text-block_018237b0485a698ad922cd10e311f9e2\" class=\"block centered-text bg_white\">\r\n\r\n    \r\n    \r\n    <div class=\"site_width\">\r\n        <div class=\"text_content\">\r\n            <p><em>The Hispanic Sociaty of America makes every attempt to ensure the accuracy of its list of supporters. To report an error or omission on this list, please contact the Development Office at (212) 926-2234, ext. 247 or<\/em><em>\u00a0<\/em><a href=\"mailto:development@hispanicsociaty.org\"><em>development@hispanicsociaty.org<\/em><\/a><em>. We sincerely apologize for any inaccuracies<\/em><\/p>\n        <\/div>\r\n        <div class=\"ctas\">\r\n                                <\/div>\r\n    <\/div>\r\n\r\n<\/div>\n\n\r\n<div id=\"related-content-block_fa02e5f736364b0448f6ca99f76f7daf\" class=\"block site_width related-content\">\r\n    <h3 class=\"related-content__heading\">Explore More<\/h3>    <div class=\"content_cards_grid\">\n                    <article class=\"single_content_card\">\n                <div class=\"content_thumbnail\">\n                    <img loading=\"lazy\" decoding=\"async\" width=\"300\" height=\"450\" src=\"https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/04\/membership_a1719-300x450.jpg\" class=\"attachment-medium size-medium wp-post-image\" alt=\"\" srcset=\"https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/04\/membership_a1719-300x450.jpg 300w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/04\/membership_a1719-800x1200.jpg 800w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/04\/membership_a1719-768x1152.jpg 768w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/04\/membership_a1719-1024x1536.jpg 1024w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/04\/membership_a1719-1365x2048.jpg 1365w, https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/04\/membership_a1719.jpg 1500w