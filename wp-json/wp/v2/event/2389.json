h: 100%;\r\n    position: relative;\r\n    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.3);\r\n    overflow: hidden;\r\n    animation: modalFadeIn 0.3s ease-out;\r\n}\r\n\r\n@keyframes modalFadeIn {\r\n    from {\r\n        opacity: 0;\r\n        transform: scale(0.9) translateY(-50px);\r\n    }\r\n    to {\r\n        opacity: 1;\r\n        transform: scale(1) translateY(0);\r\n    }\r\n}\r\n\r\n.cta-modal-header {\r\n    position: relative;\r\n    padding: 15px 20px 0;\r\n    display: flex;\r\n    justify-content: flex-end;\r\n}\r\n\r\n.cta-modal-close {\r\n    background: none;\r\n    border: none;\r\n    font-size: 24px;\r\n    cursor: pointer;\r\n    color: #666;\r\n    line-height: 1;\r\n    padding: 5px;\r\n    transition: color 0.2s ease;\r\n}\r\n\r\n.cta-modal-close:hover {\r\n    color: #333;\r\n}\r\n\r\n.cta-modal-content {\r\n    padding: 0 30px 30px;\r\n    overflow-y: auto;\r\n    max-height: calc(90vh - 60px);\r\n}\r\n\r\n.cta-modal-content div[id^=\"eventbrite-widget-container\"] {\r\n    height: 765px!important;\r\n}\r\n\r\n\/* Button styling to match existing CTA buttons *\/\r\n.cta_button.open-modal {\r\n    cursor: pointer;\r\n    border: none;\r\n    \/* Ensure button behaves like existing CTA buttons *\/\r\n}\r\n\r\n\/* Responsive adjustments *\/\r\n@media (max-width: 768px) {\r\n    .cta-modal {\r\n        max-width: 95%;\r\n        margin: 10px;\r\n    }\r\n    \r\n    .cta-modal-content {\r\n        padding: 0 20px 20px;\r\n    }\r\n}\r\n\r\n\r\n<\/style>\r\n\r\n<script>\r\ndocument.addEventListener('DOMContentLoaded', function() {\r\n    \/\/ Open modal functionality\r\n    const openModalButtons = document.querySelectorAll('.open-modal');\r\n    \r\n    openModalButtons.forEach(function(button) {\r\n        button.addEventListener('click', function(e) {\r\n            e.preventDefault(); \/\/ Prevent any default behavior\r\n            \r\n            const modalId = this.getAttribute('data-modal');\r\n            const modal = document.getElementById(modalId);\r\n            \r\n            if (modal) {\r\n                modal.style.display = 'flex';\r\n                document.body.style.overflow = 'hidden'; \/\/ Prevent background scrolling\r\n            }\r\n        });\r\n    });\r\n    \r\n    \/\/ Close modal functionality\r\n    const closeModalButtons = document.querySelectorAll('.cta-modal-close');\r\n    closeModalButtons.forEach(function(button) {\r\n        button.addEventListener('click', function() {\r\n            const modalId = this.getAttribute('data-modal');\r\n            const modal = document.getElementById(modalId);\r\n            if (modal) {\r\n                modal.style.display = 'none';\r\n                document.body.style.overflow = ''; \/\/ Restore scrolling\r\n            }\r\n        });\r\n    });\r\n    \r\n    \/\/ Close modal when clicking on overlay\r\n    const modalOverlays = document.querySelectorAll('.cta-modal-overlay');\r\n    modalOverlays.forEach(function(overlay) {\r\n        overlay.addEventListener('click', function(e) {\r\n            if (e.target === this) {\r\n                this.style.display = 'none';\r\n                document.body.style.overflow = ''; \/\/ Restore scrolling\r\n            }\r\n        });\r\n    });\r\n    \r\n    \/\/ Close modal with Escape key\r\n    document.addEventListener('keydown', function(e) {\r\n        if (e.key === 'Escape') {\r\n            const visibleModals = document.querySelectorAll('.cta-modal-overlay[style*=\"flex\"]');\r\n            visibleModals.forEach(function(modal) {\r\n                modal.style.display = 'none';\r\n                document.body.style.overflow = ''; \/\/ Restore scrolling\r\n            });\r\n        }\r\n    });\r\n});\r\n<\/script>\n\n\r\n<div id=\"text-sections-block_e1ca4885276e43f23ed55c6a6ca31aa0\" class=\"block text-sections\">\r\n            <div class=\"text-section site_width one_cols\">\r\n                            <h2 class=\"section_title\">\r\n                    Tours at Two                <\/h2>\r\n            \r\n                            <div class=\"text_content\">\r\n                    <p>Join us every Friday and Saturday at 2pm for a free guided tour of the Hispanic Sociaty Museum &amp; Library. Learn about the museum&#8217;s history and explore works on view, special collections, and the landmarked Audubon terrace. Tours are led by museum educators and last approximately 45-60 minutes<\/p>\n                <\/div>\r\n                    <\/div>\r\n    <\/div>\r\n\r\n","protected":false},"excerpt":{"rendered":"","protected":false},"featured_media":1694,"template":"","meta":{"_acf_changed":true},"event-category":[28],"class_list":["post-2389","event","type-event","status-publish","has-post-thumbnail","hentry","event-category-guided-tour"],"acf":[],"yoast_head":"<!-- This site is optimized with the Yoast SEO plugin v26.0 - https:\/\/yoast.com\/wordpress\/plugins\/seo\/ -->\n<title>Tours at Two - Hispanic Sociaty Museum and Library<\/title>\n<meta name=\"robots\" content=\"index, follow, max-snippet:-1, max-image-preview:large, max-video-preview:-1\" \/>\n<link rel=\"canonical\" href=\"https:\/\/hispanicsociaty.org\/event\/tours-at-two\/\" \/>\n<meta property=\"og:locale\" content=\"en_US\" \/>\n<meta property=\"og:type\" content=\"article\" \/>\n<meta property=\"og:title\" content=\"Tours at Two - Hispanic Sociaty Museum and Library\" \/>\n<meta property=\"og:url\" content=\"https:\/\/hispanicsociaty.org\/event\/tours-at-two\/\" \/>\n<meta property=\"og:site_name\" content=\"Hispanic Sociaty Museum and Library\" \/>\n<meta property=\"article:publisher\" content=\"https:\/\/www.facebook.com\/hispanicsociaty\" \/>\n<meta property=\"article:modified_time\" content=\"2025-09-22T15:42:53+00:00\" \/>\n<meta property=\"og:image\" content=\"https:\/\/hispanicsociaty.org\/wp-content\/uploads\/2025\/05\/HSAOpeningEstrado5-1200x800.jpg\" \/>\n\t<meta property=\"og:image:width\" content=\"1200\" \/>\n\t<meta property=\"og:image:he